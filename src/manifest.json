{
  "manifest_version": 2,
  "name": "Regexp Search Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["http://localhost/*"],
      "js": ["src/content.js"]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.jpg"
  }
}
